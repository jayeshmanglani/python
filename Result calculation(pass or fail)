print("Enter marks of 5 subjects")
m=int(input("Marks in Math out of 10: "))
e=int(input("Marks in English out of 10: "))
s=int(input("Marks in Science out of 10: "))
c=int(input("Marks in Computer out of 10: "))
h=int(input("Marks in Hindi out of 10: "))
add=(m+e+s+c+h)/50
p=add*100
print("Percentage of student: ",p,"%")

if(p>33):
    print("\nPASS")
else:
    print("\nFAIL")
